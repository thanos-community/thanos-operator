{
  "kind": "Deployment",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "thanos-query",
    "namespace": "monitoring",
    "creationTimestamp": null
  },
  "spec": {
    "selector": {
      "matchLabels": {
        "app": "thanos-query"
      }
    },
    "template": {
      "metadata": {
        "creationTimestamp": null,
        "labels": {
          "app": "thanos-query"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "thanos",
            "image": "quay.io/thanos/thanos:v0.35.1",
            "args": [
              "query",
              "--log.level=info"
            ],
            "resources": {
              "limits": {
                "cpu": "1",
                "memory": "1Gi"
              },
              "requests": {
                "cpu": "500m",
                "memory": "512Mi"
              }
            }
          }
        ]
      }
    },
    "strategy": {}
  },
  "status": {}
}