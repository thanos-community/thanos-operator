{
  "kind": "StatefulSet",
  "apiVersion": "apps/v1",
  "metadata": {
    "name": "thanos-store",
    "namespace": "monitoring"
  },
  "spec": {
    "selector": {
      "matchLabels": {
        "app": "thanos-store"
      }
    },
    "template": {
      "metadata": {
        "labels": {
          "app": "thanos-store"
        }
      },
      "spec": {
        "containers": [
          {
            "name": "thanos",
            "image": "quay.io/thanos/thanos:v0.39.0",
            "args": [
              "store",
              "--log.level=info"
            ],
            "resources": {
              "limits": {
                "cpu": "2",
                "memory": "2Gi"
              },
              "requests": {
                "cpu": "1",
                "memory": "1Gi"
              }
            }
          }
        ]
      }
    },
    "serviceName": "",
    "updateStrategy": {}
  },
  "status": {
    "replicas": 0,
    "availableReplicas": 0
  }
}